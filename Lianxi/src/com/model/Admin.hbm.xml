<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="com.model.Admin" table="admin" lazy="false">
    <id name="adminId" type="java.lang.Integer" column="admin_id">
        <generator class="identity" />
    </id>

    <property
        name="adminName"
        type="java.lang.String"
        column="admin_name"
        length="255"
    />
    <property
        name="adminPassword"
        type="java.lang.String"
        column="admin_password"
        length="255"
    />

</class>
</hibernate-mapping>
